datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

generator client {
  provider = "prisma-client-js"
}

generator json {
  provider = "prisma-json-types-generator"
}

model List {
  id   Int    @id @default(autoincrement())
  name String

  /// [RowsType]
  rows Json[] @default(value: [])

  /// [ContentType]
  storage Json[] @default(value: [])
}
