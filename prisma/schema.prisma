datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

generator json {
  provider = "prisma-json-types-generator"
}

model List {
  id   Int    @id @default(autoincrement())
  name String

  /// [ListData]
  contentsData Json
}

model User {
  id       String @id @default(cuid())
  username String
  email    String
}
