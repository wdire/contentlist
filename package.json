    "@types/lodash.debounce": "^4.0.9",
    "axios": "^1.6.7",
    "cloudinary": "^2.0.3",
    "clsx": "^2.1.0",
    "dotenv": "^16.4.5",
    "framer-motion": "^11.0.3",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "html2canvas": "^1.4.1",
    "lodash.debounce": "^4.0.8",
