{
  "name": "contentlist",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prebuild": "npx tsx ./scripts/check-envs.ts",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint --fix",
    "format:write": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json}\"",
    "format:check": "prettier --check \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json}\"",
    "prisma": "prisma",
    "prisma:gen": "prisma generate",
    "prisma:seed": "tsx ./prisma/seed.ts",
    "db:push": "prisma db push",
    "migrate:dev": "prisma migrate dev",
    "migrate:reset": "prisma migrate reset",
    "migrate:deploy": "prisma migrate deploy",
    "tsc": "npx tsc --noEmit",
    "postinstall": "prisma generate && patch-package",
    "generate-types:anilist": "npx graphql-code-generator --config ./scripts/anilist-ts-gen.ts && prettier --write \"./src/services/anilistApi/**/*.{ts,tsx}\" && eslint --fix ./src/services/anilistApi"
  },
